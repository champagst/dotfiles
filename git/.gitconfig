[include]
   path = ~/.gitconfig.local

[user]
   user = ggeise
   name = Steve Champagne
   email = champagst@gmail.com

[core]
   excludesfile = ~/.gitignore_global
   editor = vim

[alias]
   aa = add --all
   st = status
   ci = commit
   br = branch
   co = checkout
   unstage = reset --hard HEAD

[pull]
	rebase = true

[push]
	default = simple
